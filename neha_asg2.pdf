Q5//Pyramid pattern-4
class a1
{
	public static void main(String[] args)
{
	for(int i=1;i<=9;i++)
	{
	for(int j=9;j>=i;j--)
	{
	System.out.print("  ");
	}
	for(int k=1;k<=i;k++)
	{
	System.out.print(k+" ");
	}
	for(int l=i-1;l>=1;l--)
	{
	System.out.print(l+" ");
	}
	System.out.println("");
	}
}}	
	
	
Q5//Pyramid pattern-5
	class a1{
	public static void main(String[] args)
{
	for(int i=1;i<=9;i++)
	{
	for(int j=1;j<=9;j++)
	{
	if(j>=10-i)
	System.out.print(j+" ");
	else
	System.out.print("  ");
	}
	for(int k=1;k<=8;k++)
	{
	if(k<i && i!=1)
	{
	int c=9-k;
	System.out.print(c+" ");
	c=0;
	}
	else
	System.out.print("  ");
	}
	System.out.println();
	}
}
}
	
	
