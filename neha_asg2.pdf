Q1 //Pyramid pattern-1
package edac;
   import java.util.*;
	class Pattern01
	{
		public static void main(String[]args)
		{
                    int i,j,k,o=9;
                    for(i=1;i<=9;i++)
                    {   
                        for(k=1;k<o;k++)
			{   
			System.out.print(" ");
			}o--;
                        
			for(j=1;j<=i;j++)
			{   
			System.out.print(i+" ");
			}
			System.out.println("");
			}
		}
	}
    
